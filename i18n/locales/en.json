  "commands": [
    {
      "name": "ping",
      "usage": "",
      "description": "Bot's ping",
      "replies": [
        {
          "key": "pong",
          "value": "Pong!"
        }
      ]
    },
    {
      "name": "help",
      "usage": "[command]",
      "description": "Shows all commands or info about a specific command",
      "replies": [
        {
          "key": "noCommand",
          "value": "Command not found"
        },
        {
          "key": "noDescription",
          "value": "No description"
        },
        {
          "key": "noUsage",
          "value": "No usage"
        },
        {
          "key": "commandInfo",
          "value": "*Command ```{{name}}``` Info*\n\n{{description}}\n\n*Usage:* ```{{usage}}````\n\n*Aliases:* ```{{aliases}}```"
        },
        {
          "key": "commandsList",
          "value": "*Commands List*\n\n{{commands}}"
        }
      ]
    },
    {
      "name": "tiktok",
      "usage": "[{{link}}] ({{type}})",
      "description": "{{download}} a tiktok video",
      "replies": [
        {
          "key": "invalidUrl",
          "value": "Invalid url, please send a valid tiktok url"
        }
      ]
    },
    {
      "name": "sticker",
      "usage": "[{{name}}]",
      "description": "Sends a sticker",
      "replies": [
        {
          "key": "noMedia",
          "value": "Please send a video/photo in caption"
        }
      ]
    },
    {
      "name": "weather",
      "usage": "({{place}}])",
      "description": "Shows weather info about a city (default: capital of your country)",
      "replies": [
        {
          "key": "noCity",
          "value": "Please send a city name"
        },
        {
          "key": "noData",
          "value": "City not found"
        },
        {
          "key": "weatherInfo",
          "value": "*Country:* {{country}}\n\n*City:* {{city}}\n\n*Status:* {{status}}\n\n*Temperature:* {{temperature}}\n\n*Humidity:* {{humidity}}\n\n*Pressure:* {{pressure}}\n\n*Wind:* {{wind}}\n\n*Sunrise:* {{sunrise}}\n\n*Sunset:* {{sunset}}\n\n*Date:* {{date}}\n\n*Timezone:* {{timezone}}\n\n*coordinates:* {{geo}}"
        }
      ]
    }
  ],
  "variables": [
    {
      "key": "download",
      "values": ["download", "send"]
    },
    {
      "key": "link",
      "values": ["link", "url", "video"]
    },
    {
      "key": "name",
      "values": ["name", "title"]
    },
    {
      "key": "place",
      "values": ["place", "location", "city", "country", "address"]
    }
  ]
